<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.lynxz.rxjavademo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.OperatorDemoActivity"/>
        <activity android:name=".activity.BasicDemoActivity"/>
        <activity android:name=".activity.ErrorHandlerActivity"/>
        <activity android:name=".activity.RxbindingActivity"/>
        <activity android:name=".activity.FilterDemoActivity"/>
        <activity android:name=".activity.RetrofitDemoActivity"/>
    </application>

</manifest>